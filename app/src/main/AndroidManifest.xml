        android:theme="@style/AppTheme">
        <activity android:name=".alram.AlarmDrug" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".drugandhopistal.MapsActivity"
            android:label="@string/title_activity_maps" />
        <activity android:name=".drugandhopistal.ScollDrug" />
        <activity android:name=".chat.BangTinActyvity" />
